#!/usr/bin/env bash

check_factor() {
    if [ $# -ne 1 ]; then
        echo "Usage: check_factor <number>"
        exit 1
    fi

    num=$1
    result=$(factor "$num")

    # Extract the prime factors from the result
    prime_factors=$(echo "$result" | awk '{print $2, $3}')

    echo "Prime factors: $prime_factors"
}

if [ $# -ne 1 ]; then
    echo 'Usage: rsa <file>'
    exit 1
else
    while read -r i; do
        check_factor "$i"
    done < "$1"
fi
